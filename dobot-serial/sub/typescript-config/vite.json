{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "Display": "Vite",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "types": [
      "vite/client",
      "node"
    ]
  }
}
